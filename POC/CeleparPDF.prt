	<!-- Checkbox Settings
	<CheckboxSize> is the checkbox size in inches. If this parameter is 0 or left out, the checkbox
		is set to the size of the font specified for the individual checkbox.
	<CheckboxShape> Square, Circle - how to draw the checkbox. Use "None" to not draw the 
		outer box (if chars from symbol fonts are used in CheckboxStyle).
	<CheckboxStyle> is how the checkbox is shown to be checked or unchecked.
		Set to Fill to fill the box as a solid color when checked (unfilled when unchecked)
		Set to Check to place a check mark in the box when checked (unfilled when unchecked)
		Set to a 1 or 2 character string to use those characters as the checked and unchecked characters.
			(If only 1 character specified, it is displayed for the checked status, unfilled for unchecked status).
	-->
	<CheckboxSize>0</CheckboxSize>
	<CheckboxShape>Square</CheckboxShape>
	<CheckboxStyle>Check</CheckboxStyle>

	<!-- RadioButton Settings
		These are identical to the Checkbox settings, but apply to RadioButton fields rather than Checkbox fields.
	-->
	<RadioButtonSize>0</RadioButtonSize>
	<RadioButtonShape>Circle</RadioButtonShape>
	<RadioButtonStyle>Fill</RadioButtonStyle>

	<!-- SplitWord - If normal word wrap creates a line (single word) that is too wide to fit in the Text Label
		this setting allows the word to be split at a non-word boundary. If set to N, the offending text will overflow
		on the right of the text label.
	-->
	<SplitWord>Y</SplitWord>

	<!-- Barcode options. -->
	<!-- Note - all non-zero barcode widths are specified in standard DocOrigin units 
	eg) .01" .01in .01mm .01cm .1pts or 1/1000000th of an inch if no units are specified. -->

	<Barcode39Text>Y</Barcode39Text>					<!-- Draw text below barcode -->
	<Barcode39DerivedText>N</Barcode39DerivedText>	<!-- Display text includes all ascii-encoded text and checksum -->
	<Barcode39CompactText>N</Barcode39CompactText>	<!-- Do we display as "*BCD*" or "*    B    C    D    *" -->
	<Barcode39Ratio>3</Barcode39Ratio>				<!-- Ratio of wide bar width to narrow bar -->
	<Barcode39AddChecksum>N</Barcode39AddChecksum>	<!-- Compute and add a checksum digit -->
	<Barcode39FullAscii>N</Barcode39FullAscii>		<!-- Convert data to ascii encoded values -->
	<Barcode39BarWidth>0</Barcode39BarWidth>          <!-- Set the width of a bar; 0 - scale to designed width -->

	<Barcode25Text>N</Barcode25Text>					<!-- Draw text below barcode -->
	<Barcode25Ratio>3</Barcode25Ratio>				<!-- Ratio of wide bar width to narrow bar -->
	<Barcode25AddChecksum>N</Barcode25AddChecksum>	<!-- Compute and add a checksum digit -->
	<Barcode25BarWidth>0</Barcode25BarWidth>          <!-- Set the width of a bar; 0 - scale to designed width -->

	<Barcode11Text>Y</Barcode11Text>				<!-- Draw text below barcode -->
	<Barcode11Ratio>0</Barcode11Ratio>				<!-- Ratio of wide bar width to narrow bar; 0 - auto calculate for designed width -->
	<Barcode11Checksum>Y</Barcode11Checksum>			<!-- Compute and add a checksum digit(s) -->
	<Barcode11NarrowBarWidth>0</Barcode11NarrowBarWidth>          <!-- Set the width of a bar; 0 - scale to designed width -->

	<Barcode128Text>Y</Barcode128Text>				<!-- Draw text below barcode -->
	<Barcode128CharacterSet>8859-1</Barcode128CharacterSet>	<!-- The encoding character set to use -->
	<Barcode128BarWidth>0</Barcode128BarWidth>        <!-- Set the width of a bar; 0 - scale to designed width -->
	<Barcode128-GS1>N</Barcode128-GS1>				<!-- Is this a GS1-128 barcode? -->
	<Barcode128-GS1CheckSum>N</Barcode128-GS1CheckSum>	<!-- Is Merge calculating the Mod-10 Checksum for this GS1-128 barcode -->
	<Barcode128Esc>N</Barcode128Esc>			<!-- Enable the use of >8 to set a GS1-128 FNC1 code -->

	<BarcodePostNetBarWidth>0</BarcodePostNetBarWidth>	<!-- Set the width of a bar; 0 - scale to designed width -->
	<BarcodePostNetRatio>1</BarcodePostNetRatio>		<!-- Set the space/bars ratio -->

	<QRCodeMode>8BIT</QRCodeMode>				<!-- Encoding mode -->
	<QRCodeCharacterSet>8859-1</QRCodeCharacterSet>	<!-- The encoding character set to use -->
	<QRCodeLevel>M</QRCodeLevel>				<!-- Error correction level -->

	<BarcodeUSIMText>Y</BarcodeUSIMText>				<!-- Display text above/below the bars -->
	<BarcodeUSIMTextBelow>N</BarcodeUSIMTextBelow>	<!-- Display text below the bars -->
	<BarcodeUSIMBarWidth>0</BarcodeUSIMBarWidth>      <!-- Set the width of a bar; 0 - scale to designed width -->

	<BarcodeAUSPOSTText>Y</BarcodeAUSPOSTText>			<!-- Display text above/below the bars -->
	<BarcodeAUSPOSTTextBelow>N</BarcodeAUSPOSTTextBelow>	<!-- Display text below the bars -->
	<BarcodeAUSPOSTBarWidth>0</BarcodeAUSPOSTBarWidth>    <!-- Set the width of a bar; 0 - scale to designed width -->

	<BarcodePharmacodeBarWidth>.5mm 1.5mm 1.2mm</BarcodePharmacodeBarWidth> <!-- Widths of narrow bar, wide bar, gap -->
	<BarcodePharmacode2TrackBarWidth>.5mm .6mm</BarcodePharmacode2TrackBarWidth> <!-- Widths of bar and gap -->

	<BarcodeEANBarWidth>0</BarcodeEANBarWidth>    <!-- Set the width of a narrowest bar; 0 - scale to designed width -->

	<BarcodeUPCBarWidth>0</BarcodeUPCBarWidth>    <!-- Set the width of a narrowest bar; 0 - scale to designed width -->

	<MaxiCodeCharacterSet>8859-1</MaxiCodeCharacterSet>	<!-- The encoding character set to use -->
	<MaxiCodeMode>auto</MaxiCodeMode>		<!-- Define which mode of MaxiCode barcode to produce -->

	<!-- Printer Driver definitions
		Each printer that is to be active requires a <Printer ...> definition. (all lines up to the </Printer> are
		part of that definition). The printer Name is the name by which the printer will be known in scripting
		and in error messages. The printer Type is the specific printer language driver that is to be used - must be
		one of the builtin driver names in Merge.
	-->

	<!-- Setting for PDF generation
		This is configured like another print driver, but with some
		PDF-specific settings.
	-->
	<Printer Name="PDF" Type="PDF">

		<!-- PDF Security
		Configure security options for generated document here.
		Either OpenPasswordRequired or ModifyPasswordAllowed or both are required to turn security ON.
		-->
		<!-- <OpenPasswordRequired>Yes</OpenPasswordRequired> -->
		<!-- <OpenPassword>YourPassword</OpenPassword> -->
		<!-- <ModifyPasswordAllowed>Yes</ModifyPasswordAllowed> -->
		<!-- <ModifyPassword>YourPassword</ModifyPassword> -->
		<!-- Acceptable values: "LowResolution", "HighResolution" or leave missing/empty for Disable -->
		<!-- <PrintCapability>HighResolution</PrintCapability> -->
		<!-- Acceptable values: "InsertDeleteRotate", "FillFields", "FillFieldsCommenting", "All" or leave missing/empty for Disable -->
		<!-- <AllowChange>All</AllowChange> -->
		<!-- <AllowCopying>Yes</AllowCopying> -->
		<!-- <AllowAccessibility>Yes</AllowAccessibility> -->

		<!-- Resident Font list
			This section tells Merge which font typefaces and styles are resident in any PDF consuming
			application (Adobe Reader). Any font that is referenced in a form that is not in this list is
			considered a candidate to be embedded in the PDF.
			Set Bold="1" if only the bold version is in the printer, Bold="0" if only non-bold, or Bold="*" if both
			bold and non-bold versions are resident in the PDF consuming application. Similarly for Italic=
		-->

		<ResidentFont Typeface="Arial" Bold="*" Italic="*" />
		<ResidentFont Typeface="Times New Roman" Bold="*" Italic="*" />
		<ResidentFont Typeface="Courier New" Bold="*" Italic="*" />

		<!-- Encryption Yes/No
			Overrides encryption settings defined in Design.
			Default is 'Yes' (encrypt if passwords set up in form).
		-->
		<!-- <Encryption>Yes</Encryption>    -->

		<!-- SynthBold controls the degree of boldness synthesized when a bold version of the font
			is not available in the configuration. When this happens, the outline of the font glyph is
			drawn with this line thickness (in points or 1/72 of an inch).
		-->
		<SynthBold>0.25</SynthBold>

		<!-- HighlightURL controls if URLs and emails are automatically formatted as underlined blue text in the output.
			If URLs or emails text has custom underline or color properties defined those are not overwritten.
			It's also possible to control this behavior via object tags.
			Default is Yes.
		-->
		<HighlightURL>Yes</HighlightURL>

		<!-- Bookmark Yes/No
			Enables the generated PDF bookmarks to be viewed.
			Default is Yes.
		-->
		<Bookmark>Yes</Bookmark>

		<!-- Blend mode for PDF transparency. See "PDF Blend Modes: Addendum" document for details.
			Possible values are:
				Normal - Selects the source color, ignoring the backdrop.
				Multiply - Multiplies the backdrop and source color values.
				Screen - Multiplies the complements of the backdrop and source color values, then complements the result.
				Overlay - Multiplies or screens the colors, depending on the backdrop color value.
				Darken - Selects the darker of the backdrop and source colors.
				Lighten - Selects the lighter of the backdrop and source colors.
				ColorDodge - Brightens the backdrop color to reflect the source color.
				ColorBurn - Darkens the backdrop color to reflect the source color.
				HardLight - Multiplies or screens the colors, depending on the source color value.
				SoftLight - Darkens or lightens the colors, depending on the source color value.
				Difference - Subtracts the darker of the two constituent colors from the lighter color.
				Exclusion - Produces an effect similar to that of the Difference mode but lower in contrast.
			Default is Multiply.
			If you do not want color blending use Normal.
		-->
		<!-- <Blend>Normal</Blend> -->

		<!-- TransparencyColor defines which color is used as a background for transparent pixels in PNG images.
			Fully transparent pixels get this color assigned.
			Default is white - '255 255 255'.
		-->
		<!-- <TransparencyColor>255 255 255</TransparencyColor> -->

		<!-- Color range which is treated as transparent when an image has transparency turned on.
			Defaults to white. Make an image transparent in Merge by setting "this._transparent=true;" in image script.
			Values represent Red1 Green1 Blue1 Red2 Green2 Blue2. All colors in range
			(Red1-Red2, Green1-Green2, Blue1-Blue2) are considered to be transparent when displayed as PDF.
		-->
		<TransparencyMask>255 255 255 255 255 255</TransparencyMask>

		<!-- SupportAlpha Yes/No
			Specifies whether alpha channel of PNG images should be used for image masking instead of TransparencyMask
				when an image has transparency turned on.
			Default is No.
		-->
		<!-- <SupportAlpha>Yes</SupportAlpha> -->

		<!-- <Outputfile> indicates where the output file is to be written. A '%' followed by a letter
			causes a text substitution at runtime, to allow dynamic creation of unique names.
			This template driven filename mechanism may be used for any of the printer drivers.
			%a - abbreviated weekday name (eg "Wed")
			%A - full weekday name (eg "Wednesday"
			%b - abbreviated month name (eg "Mar")
			%B - full month name (eg "March"
			%d - replace with the 2digit current day
			%f - replace with the current form name.
			%H - replace with the 2digit current hour (local time, 24 hour clock)
			%I - replace with the 2digit current hour (local time, 12 hour clock)
			%j - replace with the 3digit day-of-year (001-366)
			%m - replace with the 2digit current month
			%M - replace with the 2digit current minute
			%p - current locale's AM/PM indicator for 12hour clock
			%S - replace with the 2digit current second
			%U - Week of year (2digit 00-53, starting on Sunday)
			%w - Weekday as number (0-6), starting on Sunday
			%W - Week of year (2digit 00-53, starting on Monday)
			%y - replace with the 2digit current year
			%Y - replace with the 4digit current year

			Note - you must include the file extension (eg .pdf) at the end

			<Outputfile>$R/PDF/%f_%Y-%m-%d_%H-%M.pdf</Outputfile>
		-->

		<!-- The filename from the <Outputfile> may not create a unique name. If the file is
			found to exist already, Merge will append an underscore followed by a digit to
			the end of the filename, incrementing the digit until a unique name is found.
			If <OverwriteFile> is set to Yes, no attempt is made to create a unique name - it will be replaced instead.
		-->
		<OverwriteFile>Yes</OverwriteFile>

		<!-- If <CombineDocuments> is set to Yes, all documents generated by this driver will be
			combined into a single multipage PDF file. Otherwise each document will create
			it's own file.
		-->
		<CombineDocuments>Yes</CombineDocuments>

		<!-- Set <EmbedFonts> to Yes to have non-resident fonts embedded in the PDF file. Otherwise
			PDF viewers will find closest match.
		-->
		<EmbedFonts>Yes</EmbedFonts>

		<!-- ViewerPreferences are acrobat display defaults that can be added to the generated PDF file.
			See the PDF Reference manual - version 1.7 (sixth edition) section 8.1 for details. (available online at Adobe and AIIM sites)
		-->
		<ViewerPreferences>/PrintClip/CropBox</ViewerPreferences>

		<!-- <Duplex> controls whether a document will be output in duplex.
			The possible settings are:
				<Duplex>form</Duplex>     - indicates to use the duplex settings that were set in the form.
				<Duplex>top</Duplex>      - indicates to print the form duplexed with top-binding no matter what is set in the form.
				<Duplex>left</Duplex>     - indicates to print the form duplexed with left-binding no matter what is set in the form.
				<Duplex>right</Duplex>    - indicates to print the form duplexed with right-binding no matter what is set in the form.
				<Duplex>simplex</Duplex>  - indicates to print the form as simplex (one-sided) no matter what is set in the form.
			The default setting for this option is form.
		-->
		<Duplex>form</Duplex>

	</Printer>

</Configuration>
