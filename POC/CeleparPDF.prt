	<Paper Name="A3" Orient="Portrait" ID="27" Width="29.7cm" Height="42cm" MTop="0" MLeft="0" MRight="0" MBottom="0"/>
	<Paper Name="A3" Orient="Landscape" ID="1027" Width="42cm" Height="29.7cm" MTop="0" MLeft="0" MRight="0" MBottom="0"/>
