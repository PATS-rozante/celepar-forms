	<!-- Some Chinese font samples
		For these fonts we set encode="CID" to for PDF Cid font selection.
		End User Defined Characters (EUDC) are defined as fonts with an eudcType of either "Default" or "Custom"
		If "Default" this EUDC font applies to any font, if "Custom" then eudcID= must be set to a unique
		ID value. This will match the EUDC font with any regular font which has the same ID= setting.
	-->
	<!-- Font TTF="simhei.ttf"  encode="CID" -->			<!-- Font: SimHei (Regular) -->
	<!-- Font TTF="simsun.ttc"  encode="CID" -->			<!-- Font: SimSun (Regular) -->
	<!-- Font TTF="Eudc.tte"    encode="CID" eudcType="Default" --> 	<!-- End User Defined Characters -->

	<!-- Page Sizes
		The <Paper .../> parameters specify the page sizes available to be printed. The form file specifies both the
		Name (eg Letter) and Orientation (eg Landscape) of a page.
		ID is a unique numeric identifier for this combination of page name and orientation. Values 1-499 are
		system assigned. Values 500-999 and above can be used for custom page sizes.
		Width/Height are the full physical page sizes (in inches).
		MTop/MLeft/MRight/MBottom are the non-printable margins on each of the 4 sides.
	-->
	<Paper Name="Letter" Orient="Portrait" ID="2" Width="8.5 in" Height="11.0 in" MTop="0" MLeft="0" MRight="0" MBottom="0"/>
	<Paper Name="Letter" Orient="Landscape" ID="1002" Width="11.0 in" Height="8.5 in" MTop="0" MLeft="0" MRight="0" MBottom="0"/>
